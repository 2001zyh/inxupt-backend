<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.allsparkstudio.zaixiyou.dao.TagMapper">
    <select id="selectByIdSet" resultMap="BaseResultMap" parameterType="java.util.Set">
        select
        <include refid="Base_Column_List" />
        from tag
        <where>
            <if test="idSet.size() > 0">
                id in
                <foreach collection="idSet" item="item" index="index" open="(" separator="," close=")">
                    #{item}
                </foreach>
            </if>
        </where>
    </select>
    <select id="selectByName" parameterType="java.lang.String" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from tag
        where name = #{name,jdbcType=VARCHAR}
    </select>
</mapper>